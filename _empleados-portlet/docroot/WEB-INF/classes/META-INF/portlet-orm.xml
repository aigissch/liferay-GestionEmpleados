<?xml version="1.0"?>

<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.0" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_1_0.xsd">
	<persistence-unit-metadata>
		<xml-mapping-metadata-complete/>
		<persistence-unit-defaults>
			<access>PROPERTY</access>
		</persistence-unit-defaults>
	</persistence-unit-metadata>
	<mapped-superclass class="com.liferay.sample.model.impl.DepartamentoModelImpl">
		<attributes>
			<id name="idDepartamento" />
			<basic name="nombre" />
			<basic name="descripcion" />
			<basic name="companyId" />
			<basic name="groupId" />
			<transient name="modelAttributes" />
			<transient name="primaryKey" />
			<transient name="primaryKeyObj" />
		</attributes>
	</mapped-superclass>
	<mapped-superclass class="com.liferay.sample.model.impl.EmpleadoModelImpl">
		<attributes>
			<id name="idEmpleado" />
			<basic name="nombre" />
			<basic name="telefono" />
			<basic name="idDepartamento" />
			<basic name="companyId" />
			<basic name="groupId" />
			<transient name="modelAttributes" />
			<transient name="primaryKey" />
			<transient name="primaryKeyObj" />
		</attributes>
	</mapped-superclass>
	<entity class="com.liferay.sample.model.impl.DepartamentoImpl" name="Departamento">
		<table name="ge_Departamento" />
		<attributes />
	</entity>
	<entity class="com.liferay.sample.model.impl.EmpleadoImpl" name="Empleado">
		<table name="ge_Empleado" />
		<attributes />
	</entity>
</entity-mappings>